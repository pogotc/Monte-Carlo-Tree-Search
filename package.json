{
  "name": "mcts",
  "version": "1.0.0",
  "description": "Monte Carlo Tree Search",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "build:watch": "onchange 'src/**/*.ts' -- npm run build",
    "lint": "./node_modules/.bin/eslint './**/*.ts'",
    "format": "./node_modules/.bin/prettier --write './**/*.ts'",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --forceExit --detectOpenHandles --verbose --maxWorkers=1",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/jest --watchAll"
  },
  "keywords": [],
